
for _, forge in forge:builds('cc_.*') do
    forge:all {
        forge:StaticLibrary '${lib}/math_lua_${architecture}' {
            forge:Cxx '${obj}/%1' {
                'LuaMat4x4.cpp',
                'LuaMath.cpp',
                'LuaVec2.cpp',
                'LuaVec3.cpp',
                'LuaVec4.cpp',
                'LuaQuat.cpp'
            };
        };
    };
end
