
for _, toolset in toolsets('cc.*') do
    toolset:all {
        toolset:StaticLibrary '${lib}/math_lua_${platform}_${architecture}' {
            toolset:Cxx '${obj}/%1' {
                'LuaMat4x4.cpp',
                'LuaMath.cpp',
                'LuaVec2.cpp',
                'LuaVec3.cpp',
                'LuaVec4.cpp',
                'LuaQuat.cpp'
            };
        };
    };
end
